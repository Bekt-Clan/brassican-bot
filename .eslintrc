{
  "root": true,
  "extends": [
    "airbnb",
    "eslint-config-prettier"
  ],
  "plugins": [
    "eslint-plugin-prettier"
  ],
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {},
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": 0,
    "strict": 0,
    "no-restricted-syntax": 0,
    "react/jsx-indent": [
      "error",
      4
    ],
    "react/jsx-indent-props": [
      "error",
      4
    ],
    "func-names": "warn",
    "max-len": 0,
    "no-console": 0,
    "no-continue": 0,
    "no-await-in-loop": 0,
    "import/extensions": 0,
    "react/require-default-props": 0,
    "camelcase": 0,
    "comma-dangle": "off",
    "eqeqeq": "error",
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ],
    "indent": "off",
    "prettier/prettier": "error"
  }
}